<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="222.0" prefWidth="516.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TypeRacer.FXControllers.PlayController">
   <children>
      <TextField fx:id="inputText" layoutX="53.0" layoutY="130.0" onKeyTyped="#keyTyped" />
      <TextArea fx:id="text" layoutX="53.0" layoutY="39.0" prefHeight="47.0" prefWidth="149.0">
         <font>
            <Font size="20.0" />
         </font></TextArea>
      <TextArea fx:id="outputText" layoutX="255.0" layoutY="39.0" prefHeight="118.0" prefWidth="236.0">
         <font>
            <Font size="20.0" />
         </font></TextArea>
      <Text layoutX="90.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Words to type" />
      <Text layoutX="68.0" layoutY="122.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Type your words here!" />
      <Text layoutX="354.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Results" />
      <Button fx:id="saveBtn" layoutX="352.0" layoutY="183.0" mnemonicParsing="false" onAction="#saveScore" text="Save" />
      <Button fx:id="backBtn" layoutX="102.0" layoutY="183.0" mnemonicParsing="false" onAction="#mainMenu" text="Back" />
   </children>
</AnchorPane>
